    "\"\"\"\n",
    "# Find doctors with the last name of 'Johnson' in the employee list. The \n",
    "output should contain both their first and last names.\n",
    "\n",
    "SELECT \n",
    "  first_name, \n",
    "  last_name \n",
    "FROM \n",
    "  employee_list \n",
    "WHERE \n",
    "  last_name = 'Johnson' AND profession = 'Doctor';\n",
    "\"\"\""
   ]
  }
